{
   "url-base":"/configuration-api/v1/",
   "endpoints":[
      {
         "url":"advisories",
         "displayValueFieldNames":[
             "ratingSystem",
             "name"
         ],
         "displayValueDelimiter" : " / ",
         "displayName":"Advisories",
         "uiSchema":[
            {
               "id":"id",
               "name":"id",
               "type":"text",
               "label":"Document Id",
               "description":"Couchbase record id",
               "disable":true
            },
            {
               "id":"createdAt",
               "name":"createdAt",
               "type":"text",
               "label":"Created At",
               "disable":true
            },
            {
               "id":"code",
               "name":"code",
               "type":"text",
               "label":"Advisory Code",
               "required":true,
               "validWhen":{
                  "matchesRegEx":{
                     "pattern":"^[\\w]+$",
                     "message":"Value must be 1 or more characters in length"
                  }
               }
            },
            {
               "id":"name",
               "name":"name",
               "type":"text",
               "label":"Advisory Definition",
               "required":true,
               "validWhen":{
                  "lengthIsGreaterThan":{
                     "length":0,
                     "message":"The advisory name must be at least one character."
                  }
               }
            },
            {
               "id":"ratingSystem",
               "name":"ratingSystem",
               "type":"select",
               "label":"Rating System",
               "required":true,
               "source":{
                  "url":"rating-systems",
                  "value":"value",
                  "label":"value"
               },
               "validWhen":{
                  "lengthIsGreaterThan":{
                     "length":0,
                     "message":"The rating system must be at least one character."
                  }
               }
            },
            {
               "id":"permutations",
               "name":"permutations",
               "type":"repeating",
               "label":"Permutations",
               "misc":{
                  "fields":[
                     {
                        "id":"permutation",
                        "name":"permutation",
                        "type":"text"
                     }
                  ],
                  "idAttribute":"permutation"
               }
            }
         ],
         "schema":{
            "name":"Advisories-UI-Schema"
         }
      },
