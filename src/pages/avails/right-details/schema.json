[
    {
        "title": "Basic Info and Territory",
        "sections": [
            {
                "title": "Basic Info",
                "fields": {
                    "rightId": {
                        "name": "Right ID",
                        "type": "string",
                        "path": "id",
                        "isReadOnly": true,
                        "viewConfig": [
                            {
                                "view": "CREATE",
                                "hidden": true
                            }
                        ]
                    },
                    "title": {
                        "name": "Title",
                        "type": "string",
                        "isRequired": true,
                        "path": "title",
                        "tooltip": "Read Only when coreTitleId exists",
                        "dependencies": [
                            {
                                "view": "EDIT",
                                "type": "readOnly",
                                "field": "coreTitleId",
                                "value": "any"
                            }
                        ]
                    },
                    "format": {
                        "name": "Format",
                        "type": "multiselect",
                        "path": "format",
                        "isRequired": true
                    },
                    "licensed": {
                        "name": "Licensed",
                        "type": "boolean",
                        "path": "licensed",
                        "viewConfig": [
                            {
                                "view": "CREATE",
                                "defaultValue": true
                            }
                        ],
                        "isReadOnly": true
                    },
                    "licensor": {
                        "name": "Licensor",
                        "type": "select",
                        "path": "licensor",
                        "isRequired": true
                    },
                    "licensee": {
                        "name": "Licensee",
                        "type": "select",
                        "path": "licensee",
                        "isRequired": true
                    },
                    "licenseType": {
                        "name": "License Type",
                        "type": "select",
                        "path": "licenseType",
                        "isRequired": true
                    },
                    "contentType": {
                        "name": "Content Type",
                        "type": "select",
                        "path": "contentType",
                        "isRequired": true
                    },
                    "releaseYear": {
                        "name": "Release Year",
                        "type": "number",
                        "path": "releaseYear",
                        "validation": [
                            {
                                "name": "lengthEqual",
                                "args": {
                                    "length": 4
                                }
                            }
                        ]
                    },
                    "allowedLanguages": {
                        "name": "Allowed Languages",
                        "type": "multiselect",
                        "path": "allowedLanguages",
                        "optionsConfig": {
                            "defaultValuePath": "languageCode",
                            "defaultLabelPath": "languageName"
                        }
                    },
                    "licenseRightsDescription": {
                        "name": "License Rights Description",
                        "type": "select",
                        "path": "licenseRightsDescription",
                        "isRequired": true
                    },
                    "platformCategory": {
                        "name": "Platform Category",
                        "type": "multiselect",
                        "path": "platformCategory",
                        "isRequired": true,
                        "optionsConfig": {
                            "options": [
                                { "label": "Residential", "value": "Residential" },
                                { "label": "Hotel", "value": "Hotel" }
                            ]
                        }
                    },
                    "rightStatus": {
                        "name": "Right Status",
                        "type": "select",
                        "path": "rightStatus",
                        "isRequired": true,
                        "optionsConfig": {
                            "options": [
                                { "label": "Pending", "value": "Pending" },
                                { "label": "Confirmed", "value": "Confirmed" },
                                { "label": "Tentative", "value": "Tentative" },
                                { "label": "Canceled", "value": "Canceled" },
                                { "label": "Withdrawn", "value": "Withdrawn" }
                            ]
                        }
                    },
                    "affiliate": {
                        "name": "Affiliate",
                        "type": "multiselect",
                        "path": "affiliate"
                    },
                    "affiliateExclude": {
                        "name": "Affiliate Exclude",
                        "type": "multiselect",
                        "path": "affiliateExclude"
                    }
                }
            },
            {
                "title": "Territory",
                "fields": {
                    "territory": {
                        "name": "Territory",
                        "type": "array",
                        "path": "territory",
                        "isRequired": true,
                        "confirmationContent": "Alternatively, you can withdraw the territory.",
                        "fields": {
                            "country": {
                                "name": "Country",
                                "path": "country",
                                "isRequired": true,
                                "className": "nexus-c-array__field-bold",
                                "type": "select",
                                "optionsConfig": {
                                    "defaultValuePath": "countryCode",
                                    "defaultLabelPath": "countryName"
                                }
                            },
                            "selected": {
                                "name": "Selected",
                                "type": "boolean",
                                "path": "selected",
                                "isReadOnly": true,
                                "className": "nexus-c-array__field-inline",
                                "viewConfig": [
                                    {
                                        "view": "CREATE",
                                        "defaultValue": false
                                    }
                                ]
                            },
                            "rightContractStatus": {
                                "name": "Right Contract Status",
                                "type": "select",
                                "path": "rightContractStatus",
                                "isRequired": true,
                                "optionsConfig": {
                                    "options": [
                                        { "label": "Pending", "value": "Pending" },
                                        { "label": "Pending Manual", "value": "PendingManual" },
                                        { "label": "Matched Once", "value": "MatchedOnce" }
                                    ]
                                }
                            },
                            "vuContractId": {
                                "name": "VU Contract ID",
                                "type": "stringInArray",
                                "path": "vuContractId"
                            },
                            "comment": {
                                "name": "Comments",
                                "type": "string",
                                "path": "comment"
                            }
                        }
                    },
                    "territoryExcluded": {
                        "name": "Territory Excluded",
                        "type": "multiselect",
                        "path": "territoryExcluded",
                        "optionsConfig": {
                            "defaultValuePath": "countryCode",
                            "defaultLabelPath": "countryName"
                        }
                    },
                    "region": {
                        "name": "Region",
                        "type": "multiselect",
                        "path": "region",
                        "optionsConfig": {
                            "defaultValuePath": "regionCode",
                            "defaultLabelPath": "regionName"
                        }
                    }
                }
            },
            {
                "title": "Content",
                "fields": {
                    "contentType": {
                        "name": "Content Type",
                        "type": "string",
                        "path": "contentType"
                    },
                    "availDates": {
                        "name": "Avail Date Range",
                        "type": "dateRange",
                        "dateType": "businessDateTime",
                        "labels": ["Avail Start", "Avail End"],
                        "path": ["availStart", "availEnd"],
                        "dependencies": [
                            {
                                "view": "EDIT",
                                "type": "required",
                                "field": "licensed",
                                "value": false
                            }
                        ]
                    },
                    "dates": {
                        "name": "Date Range",
                        "type": "dateRange",
                        "dateType": "businessDateTime",
                        "labels": ["Start", "End"],
                        "path": ["start", "end"],
                        "dependencies": [
                            {
                                "view": "EDIT",
                                "type": "readOnly",
                                "field": "licensed",
                                "value": false
                            }
                        ]
                    }
                }
            }
        ]
    },
