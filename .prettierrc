{
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "es5",
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "bracketSpacing": false,
                "arrowParens": "avoid"
            }
        },
        {
            "files": "*.scss",
            "options": {
                "parser": "scss",
                "semi": true
            }
        }
    ]
}
